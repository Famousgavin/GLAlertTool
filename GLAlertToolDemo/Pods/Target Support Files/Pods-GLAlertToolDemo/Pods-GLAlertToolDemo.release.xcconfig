GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/GLLanguageTool" "${PODS_ROOT}/Headers/Public/GLScanner" "${PODS_ROOT}/Headers/Public/MBProgressHUD"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/GLLanguageTool" "$PODS_CONFIGURATION_BUILD_DIR/GLScanner" "$PODS_CONFIGURATION_BUILD_DIR/MBProgressHUD"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/GLLanguageTool" -isystem "${PODS_ROOT}/Headers/Public/GLScanner" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD"
OTHER_LDFLAGS = $(inherited) -ObjC -l"GLLanguageTool" -l"GLScanner" -l"MBProgressHUD" -framework "CoreGraphics" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
